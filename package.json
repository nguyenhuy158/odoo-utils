{
    "name": "odoo-utils",
    "displayName": "Odoo Utils",
    "description": "Utility tools for Odoo development - check duplicate function definitions in Python files",
    "repository": {
        "type": "git",
        "url": "https://github.com/nguyenhuy158/odoo-utils.git"
    },
    "publisher": "nguyenhuy158",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.102.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "odoo-utils.checkDuplicates",
                "title": "Odoo-utils: Check Duplicate Functions"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test",
        "pub:patch": "npm test && npx vsce publish patch",
        "pub:minor": "npm test && npx vsce publish minor",
        "pub:major": "npm test && npx vsce publish major",
        "git:tag": "git push origin --tags"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.18.11",
        "@types/vscode": "^1.102.0",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "esbuild": "^0.25.11",
        "eslint": "^9.36.0",
        "typescript": "^5.9.3"
    },
    "dependencies": {}
}
